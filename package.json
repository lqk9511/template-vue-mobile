{
  "name": "template-vue-mobile",
  "author": {"name": "Jeff9511"},
  "version": "1.0.0",
  "description": "mobile vue template support typescript",
  "keywords": ["vue","mobile","template","typescript"],
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix ",
    "build:pre": "vue-cli-service build --mode staging",
    "build:test": "vue-cli-service build --mode test",
    "commit": "git cz",
    "format": "prettier --write ",
    "lint:scss": "stylelint --fix --syntax scss",
    "lint:ts": "tslint --fix"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "commitizen": "^4.0.3",
    "cz-customizable": "^6.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "yarn run format",
      "git add"
    ],
    "*.scss": [
      "yarn run lint:scss",
      "yarn run format",
      "git add"
    ],
    "*.{js,vue,ts,tsx}": [
      "yarn run lint",
      "yarn run format",
      "git add"
    ]
  }
}
